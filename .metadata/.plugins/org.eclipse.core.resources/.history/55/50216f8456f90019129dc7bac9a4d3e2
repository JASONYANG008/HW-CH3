package P27;

public class P27
{
	public static void main(String[] arg)
	{
		Car car1;
		car1= new Car();
		car1.show();
		
		Car car2;
		car2= new Car();
		car2.show();
	}
}
class Car {
	private int num;
	private double gas;
	
	public Car()
	{
		num=0;
		gas=0.0;
		System.out.println("a生了子");
	}
	public Car(int n,double g)
	{
		num=n;
		gas=g;
		System.out.println("生a了"+num+",汽油量"+gas+"的子");
	}
	public void show()
	{
		System.out.println("是"+num);
		System.out.println("汽油量是"+gas);
	}
}
