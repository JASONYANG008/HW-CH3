package P24;

public class P24 
{
	public static void main(String[] arg)
	{
		Car car1;
		car1 = new Car();
		
		car1.setCar(1234,20.5);
		car1.show();
		
		System.out.println("只更");
		car1.setCar(2345);
		car1.show();
		
		System.out.println("只更汽油量");
		car1.setCar(30.5);
		car1.show();
	}
}

class Car
{
	private int num;
	private double gas;
	
	public void setCar(int n) 
	{
		num =n;
		System.out.println("⑵油量O"+gas);
	}
	public void setCar(int n,double g) 
	{
		num =n;
		gas =g;
		System.out.println("④O"+num+"汽油量O"+gas);
	}
	public void show()
	{
		System.out.println("是"+this.num);
		System.out.println("汽油量是"+this.gas);
	}
}
